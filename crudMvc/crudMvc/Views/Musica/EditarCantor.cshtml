@{
    var cantor = ViewBag.Cantor;
}
<title>Editar Cantor</title>
<form action="Put" method="post">
    <input type="hidden" value="@cantor.Id" name="Id" id="nome"/>
    <label for="nome">Nome do Cantor</label>
    <input type="text" value="@cantor.NomeCantor" name="NomeCantor" id="nome" />
    <label for="nome">Nome da Musica</label>
    <input type="text" value="@cantor.NomeMusica" name="NomeMusica" id="nome" />

    <button type="submit"> Atualizar</button>
    
</form>